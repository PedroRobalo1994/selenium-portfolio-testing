{
  "name": "portfolio-selenium-testing",
  "version": "1.0.0",
  "description": "Automated Selenium testing for Pedro Robalo's portfolio website",
  "main": "index.js",
  "scripts": {
    "test": "jest --detectOpenHandles",
    "test:watch": "jest --watch --detectOpenHandles",
    "test:coverage": "jest --coverage --detectOpenHandles",
    "generate-report": "node src/utils/reportGenerator.js",
    "start:dashboard": "serve docs",
    "lint": "eslint src/**/*.js",
    "format": "prettier --write src/**/*.js"
  },
  "dependencies": {
    "selenium-webdriver": "^4.15.0",
    "jest": "^29.7.0",
    "chromedriver": "^119.0.1"
  },
  "devDependencies": {
    "eslint": "^8.54.0",
    "prettier": "^3.1.0",
    "serve": "^14.2.1"
  },
  "keywords": ["selenium", "testing", "automation", "portfolio"],
  "author": "Pedro Robalo",
  "license": "MIT"
}